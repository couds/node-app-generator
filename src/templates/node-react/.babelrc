{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead",
        "useBuiltIns": "entry", // alternative mode: "entry"
        "corejs": 3 // default would be 2
      }
    ]
  ],
  "sourceType": "unambiguous",
  "plugins": [
    "babel-plugin-styled-components",
    "babel-plugin-macros",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}